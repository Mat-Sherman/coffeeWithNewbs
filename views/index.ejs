
<% include partials/header %>
 

 <div class="container">
     <header class="jumbotron" style="text-align: center">
         <div class="container">
             <h1>#yesphx</h1>
             <h2> Browse these community members who want to meet you.  Click the "more info"  button and learn more about everyone listed!</h2>
             <br>
             <br>
             
        <a href="/register" class="btn btn-primary btn-lg" id="makeAnAccount">Make an Account! :)</a> 
        
             <script type="text/javascript">
    mixpanel.track_links("#makeAnAccount", "Make an account pressed", {
        "referrer": document.referrer
    });
</script>
</div>

             <br>
              <br>
              
              <a href="https://www.facebook.com/groups/2016337025260060/" target="_blank" class="btn btn-info" id="facebookCommunity">Join our facebook community!</a>
              
              <script type="text/javascript">
    mixpanel.track_links("#facebookCommunity", "Join Facebook Community", {
        "referrer": document.referrer
    });
</script>
              <br>
              <br>
             <a href ="/faq" class="btn btn-info" id="faq">FAQ's?</a>
             
              <script type="text/javascript">
    mixpanel.track_links("#faq", "Faq's Pressed", {
        "referrer": document.referrer
    });
</script>
              <br>
             
            
             
             <br>
             <br>
             
             
             
             <br>
             <div style="height:0px;overflow:hidden">
   
             <br>
             
             <p>
              <!--  <a class="btn btn-primary btn-large" href="/community/new">Add yourself</a> -->
             </p>
         </div>
     </header>
     
     
     
     
     
     
     <div class="row text-center" style="display:flex; flex-wrap: wrap;">
        <% community.forEach(function(community){ %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                   <img src="<%= community.image %>">
                   <div class="caption">
                       <h3><%= community.name %></h3>
                   </div>
                  
                       
                       <h4> <%=community.desc %></h4>
             
                    <p> 
                <a href="/community/<%= community._id %>" class="btn btn-primary" id="moreInfoPressed">More Info</a>
                
                 <script type="text/javascript">
    mixpanel.track_links("#moreInfoPressed", "More info pressed", {
        "referrer": document.referrer
    });
</script>
                
                </p>
                
                </div>
            </div>
        <% }); %>
    </div>
</div>
           
          
       
    
      

<a href="/">Go Back</a>


<% include partials/footer %>